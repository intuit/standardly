{
  "name": "standardly",
  "version": "0.1.0",
  "description": "DIY framework for Standards Governance",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "nyc --reporter=cobertura mocha './test/unit/*.js'",
    "test:integration": "mocha './test/integration/*.js'",
    "lint": "node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/standardly.git"
  },
  "keywords": [
    "standards",
    "governance",
    "compliance"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "app-root-path": "2.1.0",
    "bunyan": "1.8.12",
    "chai-arrays": "2.0.0",
    "colors": "1.3.3",
    "commander": "2.19.0",
    "figlet": "1.2.1",
    "fs-extra": "7.0.1",
    "js-yaml": "3.13.0",
    "json2csv": "4.2.1",
    "request": "2.88.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "csvtojson": "2.0.8",
    "eslint": "5.12.1",
    "eslint-plugin-mocha": "5.2.0",
    "husky": "1.3.1",
    "mocha": "5.2.0",
    "nyc": "13.3.0",
    "shelljs": "0.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/eslint --fix . && npm test && npm run test:integration"
    }
  }
}
